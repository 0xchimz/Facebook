{
  "name": "Facebook-Comment",
  "version": "0.0.0",
  "homepage": "https://github.com/chinclubi/Facebook",
  "authors": [
    "ChinCluBi <chincub@gmail.com>"
  ],
  "moduleType": [
    "node"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "1.3.16",
    "bootstrap-sass": "~3.3.5",
    "jquery": "~2.1.4",
    "bootstrap-social": "~4.9.1"
  },
  "install" : {
    "base" : "assets",
    "path" : {
      "js"  : "/js",
      "eot" : "/fonts",
      "svg" : "/fonts",
      "ttf" : "/fonts",
      "woff": "/fonts",
      "woff2": "/fonts",
      "otf": "/fonts",
      "scss" : "/stylesheets"
    },
    "sources" : {
      "angular" : "bower_components/angular/angular.min.js",
      "angular-route" : "bower_components/angular-route/angular-route.min.js",
      "jquery" : "bower_components/jquery/dist/jquery.min.js",
      "bootstrap" : ["bower_components/bootstrap/fonts/*"],
      "bootstrap-sass" : 
        [
          "bower_components/bootstrap-sass/assets/stylesheets/**"
        ],
      "font-awesome" : 
        [ 
          "bower_components/font-awesome/fonts/*",
          "bower_components/font-awesome/scss/*"
        ],
      "bootstrap-social" : "bower_components/bootstrap-social/bootstrap-social.scss"
    }
  }
}
